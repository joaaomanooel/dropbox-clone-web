{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "airbnb",
    "react-app"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "import",
    "jsx-a11y"
  ],
  "rules": {
    "no-underscore-dangle": 0,
    "react/prop-types": 0,
    "react/jsx-filename-extension": 0,
    "react/destructuring-assignment": 1
  }
}
